{
    "parserOptions": {
        "ecmaVersion": 2016
    },
    "env": {
        "node": true
    },
    "extends": [
        "airbnb"
    ],
    "rules": {
        "no-console": 0,
        "comma-dangle": ["error", {
            "arrays": "always-multiline",
            "objects": "always-multiline",
            "imports": "always-multiline",
            "exports": "always-multiline",
            "functions": "ignore",
        }]
    }
}
